'use strict';

angular.module('angfullsApp')
  .controller('GamesCtrl', function ($scope, $http) {
    $http.get('/api/games')
    .success(function(data) {
      $scope.games = data;
      console.log($scope.games);
    })
    .error(function(err) {
      alert('Error! Something went wrong');
    });
  });

